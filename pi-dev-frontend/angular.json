{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pi-dev-frontend": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/pi-dev-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "src/assets/fonts",
                "output": "/assets/fonts"
              }
            ],
            "styles": [
              "src/styles.css",
              "@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": [
              {"input": "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js", "bundleName": "bootstrap-bundle", "inject": true},
              {"input": "src/assets/js/jquery.min.js", "bundleName": "jquery", "inject": true},
              {"input": "src/assets/js/popper.min.js", "bundleName": "popper", "inject": true},
              {"input": "src/assets/js/bootstrap.min.js", "bundleName": "bootstrap", "inject": true},
              {"input": "src/assets/js/owl.carousel.min.js", "bundleName": "owl-carousel", "inject": true},
              {"input": "src/assets/js/jquery.magnific-popup.min.js", "bundleName": "magnific-popup", "inject": true},
              {"input": "src/assets/js/wow.min.js", "bundleName": "wow", "inject": true},
              {"input": "src/assets/js/jquery.scrollup.min.js", "bundleName": "scrollup", "inject": true},
              {"input": "src/assets/js/jarallax.min.js", "bundleName": "jarallax", "inject": true},
              {"input": "src/assets/js/jarallax-video.min.js", "bundleName": "jarallax-video", "inject": true},
              {"input": "src/assets/js/classynav.js", "bundleName": "classynav", "inject": true},
              {"input": "src/assets/js/jquery.sticky.js", "bundleName": "sticky", "inject": true},
              {"input": "src/assets/js/active.js", "bundleName": "active", "inject": true},
              {"input": "src/assets/js/core/popper.min.js", "bundleName": "core-popper", "inject": true},
              {"input": "src/assets/js/core/bootstrap.min.js", "bundleName": "core-bootstrap", "inject": true},
              {"input": "src/assets/js/plugins/perfect-scrollbar.min.js", "bundleName": "perfect-scrollbar", "inject": true},
              {"input": "src/assets/js/plugins/smooth-scrollbar.min.js", "bundleName": "smooth-scrollbar", "inject": true}
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "pi-dev-frontend:build:production"
            },
            "development": {
              "browserTarget": "pi-dev-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pi-dev-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
